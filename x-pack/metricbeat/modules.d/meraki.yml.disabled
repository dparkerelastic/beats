# Module: meraki
# Docs: https://www.elastic.co/guide/en/beats/metricbeat/main/metricbeat-module-meraki.html

- module: meraki
  metricsets: ["device_status","device_uplinks_loss_and_latency","licenses_overview"]
  enabled: true
  period: 60s
  hosts: ["https://api.meraki.com"]
  # This can be used for token based authorization:
  bearer_token_file: /var/run/secrets/meraki/token
  headers:
    Accept: application/json  
  apiKey: <token>
  organizations: ["<number>"]  
